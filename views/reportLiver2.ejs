                $("#rate0,#rate1,#rate3,#rate4,#rate5,#rate6").css("visibility", "hidden")
            } else if (dnarate == 2) {
                $("#rate3").css("visibility", "visible")
                $("#rate0,#rate1,#rate2,#rate4,#rate5,#rate6").css("visibility", "hidden")
            } else if (dnarate == 3) {
                $("#rate4").css("visibility", "visible")
                $("#rate0,#rate1,#rate2,#rate3,#rate5,#rate6").css("visibility", "hidden")
            } else if (dnarate == 4) {
                $("#rate5").css("visibility", "visible")
                $("#rate0,#rate1,#rate2,#rate3,#rate4,#rate6").css("visibility", "hidden")
            } else if (dnarate >= 5) {
                $("#rate6").css("visibility", "visible")
                // $("input[name=livercancerrate]").attr("value", "92%-97%")
                $("#rate0,#rate1,#rate2,#rate3,#rate4,#rate5").css("visibility", "hidden")
            }
        })

        $("input[id=submit]").click(function () {
            alert("xxxx")
            $('input').attr('value', function () {
                return $(this).val();
            });
            var finalHtml = {}
            finalHtml.dnarate = parseInt($('#dnarate').val())
            finalHtml.id = '<%=data.id%>'
            finalHtml.sampleid = '<%=data.sampleid%>'
            finalHtml.reportPage = "<html>" + $('html').html() + "</html>"
            $.post("/admin/saveReport", finalHtml, function (data) {
                if (data == "success") alert("保存成功")

            })
        })
        $("input[id=generate_pdf]").click(function () {
            var pdf = {}
            pdf.id = '<%=data.id%>'
            pdf.sampleid = '<%=data.sampleid%>'
            $.post("/admin/buildliverpdf", pdf, function (data) {
                if (data == "success") alert("pdf generated success")
            })
        })

        })
      
    </script>

</body>

